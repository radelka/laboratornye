<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuPersonController">
   <children>
      <TableView fx:id="catalogTable" prefHeight="400.0" prefWidth="375.0">
        <columns>
          <TableColumn fx:id="productId" prefWidth="99.0" text="ID" />
          <TableColumn fx:id="productName" prefWidth="92.0" text="Название" />
            <TableColumn fx:id="productSum" prefWidth="96.0" text="Цена" />
            <TableColumn fx:id="productCount" prefWidth="90.0" text="Кол-во" />
        </columns>
      </TableView>
      <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleMenu" text="Меню" />
      <Button fx:id="btnMenu" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#addTovar" text="Заказать" />
   </children>
</AnchorPane>
